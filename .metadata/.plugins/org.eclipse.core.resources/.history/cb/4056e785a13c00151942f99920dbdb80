package Edd;

public class Personaje {
	Personaje siguiente;
	Personaje anterior;
	String nombre;
	String tipo;
	
	Personaje(){
		this.siguiente = null;
		this.anterior = null;
	}
	Personaje(String nombre){
		this.nombre=nombre;
		this.siguiente = null;
		this.anterior = null;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	

}
