package Edd;

public class ListVertical {
NodoMatriz primero;
NodoMatriz ultimo;




public ListVertical() {
	this.primero = null;
	this.ultimo = null;
}
public NodoMatriz getPrimero() {
	return primero;
}
public void setPrimero(NodoMatriz primero) {
	this.primero = primero;
}
public NodoMatriz getUltimo() {
	return ultimo;
}
public void setUltimo(NodoMatriz ultimo) {
	this.ultimo = ultimo;
}


}
